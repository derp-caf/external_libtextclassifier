namespace libtextclassifier3.VariantValue_;
enum Type : int {
  NONE = 0,
  INT_VALUE = 1,
  INT64_VALUE = 2,
  FLOAT_VALUE = 3,
  DOUBLE_VALUE = 4,
  BOOL_VALUE = 5,
  STRING_VALUE = 6,
}

namespace libtextclassifier3;
table VariantValue {
  type:VariantValue_.Type;
  int_value:int;
  int64_value:long;
  float_value:float;
  double_value:double;
  bool_value:bool;
  string_value:string;
}

namespace libtextclassifier3;
table NamedVariant {
  name:string;
  value:VariantValue;
}

